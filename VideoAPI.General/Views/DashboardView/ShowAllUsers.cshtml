@model IEnumerable<VideoAPI.Models.Models.EmployeeModel>
@{
    ViewData["Title"] = "All users";
}


<div class="row m-3">
    <div class="col text-center">
        <h2>Список всех пользователей</h2>
    </div>
</div>

@if (TempData["SM"] != null) {
    <div class="row text-center alert alert-success fade show">
        <div class="col">
            @TempData["SM"]
        </div>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
}


<div class="row mt-2">
    <div class="col">
        <table class="table table-striped">
            <thead class="table-info">
                <tr>
                    <th>@Html.DisplayNameFor(x => x.Id)</th>
                    <th>@Html.DisplayNameFor(x => x.Login)</th>
                    <th>@Html.DisplayNameFor(x => x.Password)</th>
                    <th>@Html.DisplayNameFor(x => x.Login)</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model) {
                <tr>
                    <td>@Html.DisplayFor(x => item.Id)</td>
                    <td>>@Html.DisplayFor(x => item.Login)</td>
                    <td>>@Html.DisplayFor(x => item.Password)</td>
                    <td>>@Html.DisplayFor(x => item.Login)</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>